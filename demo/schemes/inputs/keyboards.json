    "inputs": [
      { "key": "password1", "component": "Password", "title": "常规" },
      { "key": "password2", "component": "Password", "title": "placeholder", "args": { "placeholder": "请输入密码" } },
      { "key": "password3", "component": "Password", "title": "width 60", "args": { "width": 60 } },
      { "key": "password4", "component": "Password", "title": "默认值", "args": { "defaultValue": "hehe" } },
      { "key": "password5", "component": "Password", "title": "最大限制", "args": { "maxLength": 3 } },
      { "key": "password6", "component": "Password", "title": "最小限制", "args": { "minLength": 3 } },
      { "key": "password7", "component": "Password", "title": "非空", "args": { "notEmpty": true } },
      { "key": "password8", "component": "Password", "title": "自动清空头尾空格", "args": { "autoTrim": true } }
    ]
  },

  {
    "key": "Input::Markdown",
